{
	"name": "cclog",
	"version": "0.0.0",
	"description": "Sync Claude Code transcripts to SQLite",
	"type": "module",
	"bin": {
		"cclog": "./src/index.ts"
	},
	"scripts": {
		"dev": "bun src/index.ts",
		"start": "bun src/index.ts",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "bun run --bun tsc --noEmit"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"prettier": "^3.7.4",
		"typescript": "^5.9.3"
	},
	"files": [
		"src"
	],
	"keywords": [
		"claude",
		"claude-code",
		"analytics",
		"sqlite"
	],
	"license": "MIT"
}
